"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\c_items\c_stunstick\c_stunstick"

	"$normalmapalphaenvmapmask" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[.03 .03 .05]"

	"$phong" "1"
	"$phongexponent" "23"
	"$phongboost" "0.20"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongwarptexture"  "models\weapons\c_items\c_stunstick\c_stunstick_phongwarp"
	"$phongfresnelranges"	"[.25 .1 1]"
	"$basemapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "1"	
	"$rimlightboost" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}